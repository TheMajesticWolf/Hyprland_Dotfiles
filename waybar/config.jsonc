		"autosens": 1,
		"bars": 10,
		"lower_cutoff_freq": 50,
		"higher_cutoff_freq": 20000,
		"method": "pipewire",
		"source": "auto",
		"stereo": true,
		"reverse": false,
		"bar_delimiter": 0,
		"monstercat": false,
		"waves": false,
		"noise_reduction": 0.77,
		"input_delay": 0,
		"hide_on_silence": true,
		"sleep_timer": 2,
		"bar_height": 500,
		
