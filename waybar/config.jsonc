		"gradient_color_3": "#cc6a34",
		"gradient_color_4": "#d18e47",
		"gradient_color_5": "#257095",
		"gradient_color_6": "#2888a0",
		"gradient_color_7": "#83d5df",
		"gradient_color_8": "#5b959c",

